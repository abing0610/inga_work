<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <typeAliases>
    <typeAlias type="com.inga.entity.User" alias="User"/>
    <typeAlias type="com.inga.entity.Tree" alias="Tree"/>
    <typeAlias type="com.inga.entity.Province" alias="Province"/>
  </typeAliases>
  <mappers>
    <!-- 映射文件的位置  -->
    <mapper resource="com/inga/mapper/User.xml" />
